@article{Hassani:2018aa,
	Author = {Hassani, M. Amine and Dur{\'a}n, Paloma and Hacquard, St{\'e}phane},
	Da = {2018/03/27},
	Date-Added = {2020-09-23 15:32:35 +0200},
