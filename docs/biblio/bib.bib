@article{10.3389/fpls.2018.00112,
	author = {Mhlongo, Msizi I. and Piater, Lizelle A. and Madala, Ntakadzeni E. and Labuschagne, Nico and Dubery, Ian A.},
	date-added = {2020-10-02 14:49:35 +0200},
