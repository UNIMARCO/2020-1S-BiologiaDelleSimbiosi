@article{Xu:2018aa,
	Author = {Xu, Jin and Zhang, Yunzeng and Zhang, Pengfan and Trivedi, Pankaj and Riera, Nadia and Wang, Yayu and Liu, Xin and Fan, Guangyi and Tang, Jiliang and Coletta-Filho, Helv{\'e}cio D. and Cubero, Jaime and Deng, Xiaoling and Ancona, Veronica and Lu, Zhanjun and Zhong, Balian and Roper, M. Caroline and Capote, Nieves and Catara, Vittoria and Pietersen, Gerhard and Verni{\`e}re, Christian and Al-Sadi, Abdullah M. and Li, Lei and Yang, Fan and Xu, Xun and Wang, Jian and Yang, Huanming and Jin, Tao and Wang, Nian},
